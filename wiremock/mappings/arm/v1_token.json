{
  "request": {
    "method": "POST",
    "headers": {
      "Content-Type": {
        "contains": "application/json"
      }
    },
    "urlPath": "/auth/account/token"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "access_token": "some-token",
      "token_type": "Bearer",
      "expires_in": "3600"
    }
  }
}
